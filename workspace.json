{
  "version": 2,
  "projects": {
    "design-system-web": {
      "projectType": "application",
      "root": "apps/design-system-web",
      "sourceRoot": "apps/design-system-web/src",
      "prefix": "baz",
      "targets": {
        "build": {
          "executor": "@angular-devkit/build-angular:browser",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "outputPath": "dist/apps/design-system-web",
            "index": "apps/design-system-web/src/index.html",
            "main": "apps/design-system-web/src/main.ts",
            "polyfills": "apps/design-system-web/src/polyfills.ts",
            "tsConfig": "apps/design-system-web/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "apps/design-system-web/src/favicon.ico",
              "apps/design-system-web/src/assets"
            ],
            "styles": [
              "libs/xplat/web/scss/src/_index.scss",
              "apps/design-system-web/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/design-system-web/src/environments/environment.ts",
                  "with": "apps/design-system-web/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "executor": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "design-system-web:build:production"
            },
            "development": {
              "browserTarget": "design-system-web:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "executor": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "design-system-web:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/design-system-web/src/**/*.ts",
              "apps/design-system-web/src/**/*.html"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/design-system-web"
          ],
          "options": {
            "jestConfig": "apps/design-system-web/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "design-system-web-e2e": {
      "root": "apps/design-system-web-e2e",
      "sourceRoot": "apps/design-system-web-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/design-system-web-e2e/cypress.json",
            "devServerTarget": "design-system-web:serve:development",
            "tsConfig": "apps/design-system-web-e2e/tsconfig.json"
          },
          "configurations": {
            "production": {
              "devServerTarget": "design-system-web:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/design-system-web-e2e/**/*.{js,ts}"
            ]
          }
        }
      }
    },
    "xplat-core": {
